{
    "name": "ucscode/easy-paginator",
    "type": "library",
    "description": "A lightweight and flexible PHP pagination library for handling paginated data and generating pagination links.",
    "keywords": [
        "paginator", 
        "pagination", 
        "pager"
    ],
    "homepage": "http://github.com/ucscode/easy-paginator",
    "license": "MIT",
    "authors": [
        {
            "name": "Uchenna Ajah",
            "email": "uche23mail@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ucscode/uss-element": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "Ucscode\\Paginator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ucscode\\Paginator\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
